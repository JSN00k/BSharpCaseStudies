<org.eventb.theory.core.theorem name="/" org.eventb.core.label="Cancellation Law:" org.eventb.core.predicate="∀T, equ, op, ident·T ∈ ℙ(T_EvB) ∧ T ↦ equ ↦ op ↦ ident ∈ Group_T(T) ⇒ (∀a, b, c·a ∈ T ∧ b ∈ T ∧ c ∈ T ⇒ ((equ(op(a ↦ c) ↦ op(b ↦ c)) = TRUE) ⇔ (equ(a ↦ b) = TRUE)))"/>
<org.eventb.theory.core.theorem name="0" org.eventb.core.label="a squared equ a implies a equ ident:" org.eventb.core.predicate="∀T, equ, op, ident·T ∈ ℙ(T_EvB) ∧ T ↦ equ ↦ op ↦ ident ∈ Group_T(T) ⇒ (∀a·a ∈ T ⇒ ((equ(op(a ↦ a) ↦ a) = TRUE) ⇒ (equ(a ↦ ident) = TRUE)))"/>
<org.eventb.theory.core.theorem name="1" org.eventb.core.label="Inverse Func:" org.eventb.core.predicate="∀T, equ, op, ident·T ∈ ℙ(T_EvB) ∧ T ↦ equ ↦ op ↦ ident ∈ Group_T(T) ⇒ (∃inv·inv ∈ T → T ∧ (∀x·x ∈ T ⇒ equ(op(inv(x) ↦ x) ↦ ident) = TRUE))"/>
