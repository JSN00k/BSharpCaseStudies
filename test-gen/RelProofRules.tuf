<org.eventb.theory.core.theoryRoot org.eventb.core.configuration="org.eventb.theory.core.thy">
<org.eventb.theory.core.proofRulesBlock name="element1" org.eventb.core.label="Sym">
<org.eventb.theory.core.metaVariable name="+" org.eventb.core.identifier="T" org.eventb.theory.core.type="ℙ(T_EvB)"/>
<org.eventb.theory.core.metaVariable name="'" org.eventb.core.identifier="x" org.eventb.theory.core.type="T_EvB"/>
<org.eventb.theory.core.metaVariable name="," org.eventb.core.identifier="y" org.eventb.theory.core.type="T_EvB"/>
<org.eventb.theory.core.metaVariable name=")" org.eventb.core.identifier="s" org.eventb.theory.core.type="ℙ(T_EvB × T_EvB × BOOL)"/>
<org.eventb.theory.core.inferenceRule name="-" org.eventb.core.label="sym" org.eventb.theory.core.applicability="interactive" org.eventb.theory.core.desc="sym">
<org.eventb.theory.core.given name="'" org.eventb.core.predicate="x∈T" org.eventb.theory.core.hyp="true"/>
<org.eventb.theory.core.given name="(" org.eventb.core.predicate="y ∈ T" org.eventb.theory.core.hyp="true"/>
<org.eventb.theory.core.given name=")" org.eventb.core.predicate="s ∈ SymetricRel_T(T)" org.eventb.theory.core.hyp="true"/>
<org.eventb.theory.core.infer name="*" org.eventb.core.predicate="s(y↦x) = TRUE"/>
<org.eventb.theory.core.given name="+" org.eventb.core.predicate="s(x↦y) = TRUE" org.eventb.theory.core.hyp="true"/>
</org.eventb.theory.core.inferenceRule>
</org.eventb.theory.core.proofRulesBlock>
<org.eventb.theory.core.importTheoryProject name="element2" org.eventb.theory.core.importTheoryProject="/test-gen">
<org.eventb.theory.core.importTheory name="'" org.eventb.theory.core.importTheory="/test-gen/Relations.dtf|org.eventb.theory.core.deployedTheoryRoot#Relations"/>
</org.eventb.theory.core.importTheoryProject>
<org.eventb.theory.core.proofRulesBlock name="element3" org.eventb.core.label="Equiv">
<org.eventb.theory.core.metaVariable name="element1" org.eventb.core.identifier="T" org.eventb.theory.core.type="ℙ(T_EvB)"/>
<org.eventb.theory.core.metaVariable name="element2" org.eventb.core.identifier="x" org.eventb.theory.core.type="T_EvB"/>
<org.eventb.theory.core.metaVariable name="element3" org.eventb.core.identifier="y" org.eventb.theory.core.type="T_EvB"/>
<org.eventb.theory.core.metaVariable name="element5" org.eventb.core.identifier="z" org.eventb.theory.core.type="T_EvB"/>
<org.eventb.theory.core.metaVariable name="element4" org.eventb.core.identifier="e" org.eventb.theory.core.type="ℙ(T_EvB × T_EvB × BOOL)"/>
<org.eventb.theory.core.inferenceRule name="element6" org.eventb.core.label="trans" org.eventb.theory.core.applicability="interactive" org.eventb.theory.core.desc="x = y, y = z =&gt; x = z">
<org.eventb.theory.core.given name="element1" org.eventb.core.predicate="x∈T" org.eventb.theory.core.hyp="false"/>
<org.eventb.theory.core.given name="element2" org.eventb.core.predicate="y ∈ T" org.eventb.theory.core.hyp="false"/>
<org.eventb.theory.core.given name="element5" org.eventb.core.predicate="z ∈ T" org.eventb.theory.core.hyp="false"/>
<org.eventb.theory.core.given name="element3" org.eventb.core.predicate="e ∈ TransRel_T(T)" org.eventb.theory.core.hyp="false"/>
<org.eventb.theory.core.given name="element4" org.eventb.core.predicate="e(x↦y) = TRUE" org.eventb.theory.core.hyp="false"/>
<org.eventb.theory.core.given name="element6" org.eventb.core.predicate="e(y↦ z) = TRUE" org.eventb.theory.core.hyp="false"/>
<org.eventb.theory.core.infer name="element7" org.eventb.core.predicate="e(x ↦ z) = TRUE"/>
</org.eventb.theory.core.inferenceRule>
<org.eventb.theory.core.inferenceRule name="element7" org.eventb.core.label="transInv" org.eventb.theory.core.applicability="interactive" org.eventb.theory.core.desc="x = y, y != z =&gt; x != z">
<org.eventb.theory.core.given name="element1" org.eventb.core.predicate="x∈T" org.eventb.theory.core.hyp="true"/>
<org.eventb.theory.core.given name="element2" org.eventb.core.predicate="y ∈ T" org.eventb.theory.core.hyp="true"/>
<org.eventb.theory.core.given name="element5" org.eventb.core.predicate="z ∈ T" org.eventb.theory.core.hyp="true"/>
<org.eventb.theory.core.given name="element3" org.eventb.core.predicate="e ∈ EquivRel_T(T)" org.eventb.theory.core.hyp="true"/>
<org.eventb.theory.core.given name="element4" org.eventb.core.predicate="e(x↦y) = TRUE" org.eventb.theory.core.hyp="true"/>
<org.eventb.theory.core.given name="element6" org.eventb.core.predicate="e(y↦ z) = FALSE" org.eventb.theory.core.hyp="true"/>
<org.eventb.theory.core.infer name="element7" org.eventb.core.predicate="e(x ↦ z) = FALSE"/>
</org.eventb.theory.core.inferenceRule>
<org.eventb.theory.core.inferenceRule name="element8" org.eventb.core.label="TransRewrite" org.eventb.theory.core.applicability="interactive" org.eventb.theory.core.desc="x = y =&gt; ((x = z) &lt;=&gt; (y = z))">
<org.eventb.theory.core.given name="element1" org.eventb.core.predicate="x∈T" org.eventb.theory.core.hyp="true"/>
<org.eventb.theory.core.given name="element2" org.eventb.core.predicate="y ∈ T" org.eventb.theory.core.hyp="true"/>
<org.eventb.theory.core.given name="element5" org.eventb.core.predicate="z ∈ T" org.eventb.theory.core.hyp="true"/>
<org.eventb.theory.core.given name="element3" org.eventb.core.predicate="e ∈ EquivRel_T(T)" org.eventb.theory.core.hyp="true"/>
<org.eventb.theory.core.given name="element4" org.eventb.core.predicate="e(x↦y) = TRUE" org.eventb.theory.core.hyp="true"/>
<org.eventb.theory.core.infer name="element7" org.eventb.core.predicate="e(x ↦ z) = e(y↦z)"/>
</org.eventb.theory.core.inferenceRule>
</org.eventb.theory.core.proofRulesBlock>
<org.eventb.theory.core.typeParameter name="element4" org.eventb.core.identifier="T_EvB"/>
</org.eventb.theory.core.theoryRoot>
